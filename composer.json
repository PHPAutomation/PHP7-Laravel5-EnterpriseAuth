    "require": {
        "laravel/framework": ">5.5",
        "microsoft/microsoft-graph": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.5",
        "silber/bouncer": "v1.0.0-rc.1",
        "owen-it/laravel-auditing": "^6.0",
        "zircote/swagger-php": "2.*",
        "DarkaOnLine/L5-Swagger": "^5.5",
