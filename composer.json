{
    "name": "metaclassing/php7-laravel5-enterpriseauth",
    "description": "Provides authentication, authorization, and accounting for enterprise laravel 5.5+ apps.",
    "require": {
        "laravel/framework": ">5.5",
        "laravel/socialite": ">3.0",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.5",
        "tymon/jwt-auth": "^0.5.12",
        "silber/bouncer": "v1.0.0-rc.1",
        "owen-it/laravel-auditing": "^6.0"
    },
    "authors": [
        {
            "name": "metaclassing",
            "email": "metaclassing@secureobscure.com"
        },
        {
            "name": "Patrick Stephan",
            "email": "patrick.stephan@metrogistics.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Metrogistics\\AzureSocialite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Metrogistics\\AzureSocialite\\ServiceProvider"
            ],
            "aliases": {
                "AzureUser": "Metrogistics\\AzureSocialite\\AzureUserFacade"
            }
        }
    }
}
